  "bugs": {
    "url": "https://github.com/snihank/React-Portfolio/issues"
  },
  "homepage": "https://github.com/snihank/React-Portfolio#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "netlify-cli": "^2.11.23"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
